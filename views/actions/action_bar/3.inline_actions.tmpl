<style>
  .qor-inline-actions {
    float: left;
  }
  .qor-inline-actions .qor-action {
    margin-right: 10px;
  }
  .qor-inline-actions a {
    line-height: 32px;
  }
</style>
{{with .Result}}
  <div class="qor-inline-actions">
    {{range $inline_action := .InlineActions }}
      <div class="qor-action">
        {{$inline_action}}
      </div>
    {{end}}
  </div>
{{end}}
